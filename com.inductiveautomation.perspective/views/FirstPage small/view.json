              "transforms": [
                {
                  "code": "\tfrom java.util import Calendar\n\t\n\tcal \u003d Calendar.getInstance()\n\tcal.set(Calendar.HOUR_OF_DAY, 19)\n\tcal.set(Calendar.MINUTE, 30)\n\tcal.set(Calendar.SECOND, 0)\n\tcal.set(Calendar.MILLISECOND, 0)\n\t\n\tstart \u003d cal.getTime()\n\t\n\tcal.add(Calendar.HOUR_OF_DAY, 13)\n\tend \u003d cal.getTime()\n\t\n\tcal.add(Calendar.HOUR_OF_DAY, -37)\n\tstart1 \u003d cal.getTime()\n\t\n\tcal.add(Calendar.HOUR_OF_DAY, 13)\n\tend1 \u003d cal.getTime()\n\t\n\treturn [\n\t    {\"itemId\": 1, \"startDate\": start, \"endDate\": end},\n\t    {\"itemId\": 2, \"startDate\": start, \"endDate\": end},\n\t    {\"itemId\": 3, \"startDate\": start, \"endDate\": end},\n\t    {\"itemId\": 1, \"startDate\": start1, \"endDate\": end1},\n\t    {\"itemId\": 2, \"startDate\": start1, \"endDate\": end1},\n\t    {\"itemId\": 3, \"startDate\": start1, \"endDate\": end1}\n\t]",
                  "type": "script"
